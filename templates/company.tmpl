  <body class="font-['Poppins']">
    <header class="py-4 container mx-auto">
      <div class="flex items-center justify-start gap-10">
        <img class="w-16" src="{{ .data.Logo }}" />
        <h1 class="text-3xl font-semibold text-gray-600">{{ .data.CompanyName }}</h1>
      </div>
    </header>
    <section class="lg:bg-cover lg:bg-no-repeat bg-contain relative" style="background-image:  url( {{ .data.BackgroundUrl}} );">
        <div class="absolute bg-black/50 h-full w-full">
        </div>
        <div class="py-16 relative z-50 container mx-auto">
            <h1 class="text-2xl font-medium text-center text-white"> {{ .data.CompanyDesc}}</h1>
            <div class="grid grid-rows-1 xl:grid-cols-{{index .gridLength 0}} lg:grid-cols-{{index .gridLength 1}} md:grid-cols-{{index .gridLength 2}} grid-cols-{{index .gridLength 3}} mx-auto py-10 w-3/4 gap-4">
            {{range $val := .data.Restaurants}}
                <a href="{{ $val.Url }}" class="flex flex-col items-center justify-between py-2 px-6 bg-white/80 gap-3 rounded-lg hover:scale-105 transition-all text-center lg:h-[210px]">
                    <h2 class="font-medium text-lg">{{ $val.Name }}</h2>
                    <p class="text-center">{{ $val.Address }}</p>
                    <button class="bg-sky-800 py-2 px-10 text-white rounded-full">
                        Bestil her
                    </button>
                </a>
            {{end}}
            </div>
        </div>
    </section>
    <div class="text-center py-2">
        <h4 class="font-semibold text-lg">Drevet af Bestil Online ApS</h4>
        <span class="font-medium">cvr. 39252945</span>
    </div>
  </body>
